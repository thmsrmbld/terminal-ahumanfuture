{
  "name": "terminal",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "autoprefixer": "^10.4.22",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "cssnano": "^7.1.2",
    "htmlhint": "^1.7.1",
    "mini-css-extract-plugin": "^2.9.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.2.0",
    "postcss-preset-env": "^10.4.0",
    "prettier": "3.6.2",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-manifest-plugin": "^6.0.1"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "start": "eleventy --serve",
    "build": "run-s clean build:assets build:site",
    "dev:assets": "env ELEVENTY_ENV=development webpack --mode production --watch",
    "dev:site": "env ELEVENTY_ENV=development eleventy --serve",
    "build:assets": "webpack --mode production",
    "build:site": "env ELEVENTY_ENV=production eleventy",
    "clean": "rm -rf ./public",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css,html}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,css,html}\"",
    "lint": "htmlhint \"src/**/*.njk\"",
    "lint:fix": "htmlhint \"src/**/*.njk\" --config .htmlhintrc"
  },
  "dependencies": {
    "reading-time": "^1.5.0",
    "remove": "^0.1.5",
    "yarn": "^1.22.22"
  }
}
