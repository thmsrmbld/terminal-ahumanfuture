---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - tagList
  addAllPagesToCollections: true
layout: base.njk
permalink: /tags/{{ tag }}/
eleventyComputed:
  title: "{{ tag | title }}"
  description: "All posts tagged “{{ tag }}”"
---
<h2>Posts in: #{{ tag }}</h2>
<div class="tags">Other tags: {% include "tags-list.njk" %} </div>
{% set postslist = collections[tag] %}
{% include "posts-list.njk" %}